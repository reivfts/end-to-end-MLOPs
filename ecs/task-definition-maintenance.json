{
  "family": "campus-services-maintenance",
  "networkMode": "awsvpc",
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "256",
  "memory": "512",
  "executionRoleArn": "arn:aws:iam::643771447281:role/ecsTaskExecutionRole",
  "executionRoleArn": "arn:aws:iam::643771447281:role/ecsTaskExecutionRole",
  "containerDefinitions": [
    {
      "name": "maintenance",
      "image": "643771447281.dkr.ecr.us-east-2.amazonaws.com/campus-services-hubs:maintenance-latest",
      "essential": true,
      "portMappings": [
        {
          "containerPort": 8080,
          "protocol": "tcp"
        }
      ],
      "environment": [],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/campus-services",
          "awslogs-region": "us-east-2",
          "awslogs-stream-prefix": "maintenance"
        }
      }
    }
  ]
}
